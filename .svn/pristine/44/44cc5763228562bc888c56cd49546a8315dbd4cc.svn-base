.store_img,.design_img,.suit_img{
	.uni-padding-wrap{
		padding: 0 22upx;
	}
	.page-section{
		border-radius: 10upx;
		overflow: hidden;
	}
	.title{
		display: flex;
		justify-content: space-between;
		padding: 0 22upx;
		margin-top: 54upx;
		.title1{
			color: #272727;
			font-size: 30upx;
			&:before{
				content: '';
				display: inline-block;
				width: 5upx;
				height: 29upx;
