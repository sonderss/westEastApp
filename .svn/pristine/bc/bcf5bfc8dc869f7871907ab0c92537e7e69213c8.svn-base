*{
	box-sizing: content-box;
}
.menu{
	height: 60upx;
	color: #555555;
	display: flex;
	align-items: flex-end;
	// margin-top: 10upx;
	padding: 0 26.5upx;
	view{
		padding: 0 13.5upx;
		font-size: 35upx;
		transition: color .3s;
	}
	.active{
		// padding: 0 27upx;
		// font-size: 36upx;
		// font-weight: bold;
		// text-decoration: underline;
		color: #d50000;
	}
}
.header{
	background: #f1f1f1;;
	width: 100%;;
	height: 82upx;
	// padding-top:45upx;
	// position: fixed;
	top: 0;
	z-index: 1;
	.search_btn{
		background: #de0303;
		color: #fff;
		width: 100upx;
		height: 50upx;
		font-size: 24upx;
		line-height: 50upx;
		white-space: nowrap;
		margin-left: 18upx;
	}
	.add_search_scan{
		display: flex;
		align-items: center;
		justify-content: center;
		height: 85upx;
		margin-top: 10upx;
		padding: 0 13.5upx;
	}
	.address{
		text-align: center;
		min-width: 15%;
		.city{
			color: #fff;
			font-size: 25upx;
			padding: 0 20upx;
		}
		.wundu{
			color: #fff;
			font-size: 25upx;
		}
	}
	.search_box{
		background: #fff;
		border-radius: 50upx;
		height: 56upx;
		line-height: 56upx;
		width: 77%;
		position:relative;
		.search_ico{
			position: absolute;
			width: 22upx;
			height: 27upx;
			top: 16upx;
			left: 20upx
		}
		.camera_ico{
			position: absolute;
			width: 27upx;
			height: 22upx;
			top: 16upx;
			right: 23upx
		}
		
		.search_input{
			position: absolute;
			width: 80%;
			height: 56upx;
			left: 9%;
			top: 0;
			line-height: 56upx;
			font-size: 25upx;
			height: 56upx;
		}
	}
	
	.scan{
		width: 86upx;
		.scan_ico{
			width: 32upx;
			height: 32upx;
			margin: 0 auto;
			display: block;
		}
	}
}
.uni-input-form{
	height: 100%;;
}
.cont{
	padding: 0 20upx 0 20upx;
	.cont1{
		margin-top: 50upx;
		position: relative;
		.h1{
			font-size: 30upx;
			color: #444444;
		}
		.delete_ico{
			position: absolute;
			width: 60upx;
			height: 60upx;
			top: 0;
			right: 0;
		}
		.history_list{
			display: flex;
			padding: 4px 0 18px;
			flex-wrap: wrap;
			view{
				margin: 8px 12px;
				background: #f4f4f4;
				border-radius: 8px;
				text-align: center;
				font-size: 14px;
				color: #000;
				height: 30px;
				line-height: 30px;
				padding: 0 11px;
			}
		}
		.relevant_list{
			display: flex;
			flex-wrap: wrap;
			width: 100%;
			margin-top: 38upx;
			.list{
				// width: ;
			}
			.img_cont{
				width: 145upx;
				margin: 0 15upx;
				margin-bottom: 20upx;
				height: 145upx;
				background: #f1f1f1;
				border-radius: 10upx;
				.img{
					height: 100%;
				}
			}
		}
	}
}
.search_guide{
	position: fixed;
	width: 100%;
	border-radius: 30upx;
	top: 0;
	background: #FFFFFF;
	height: 100%;
	padding: 0 60upx;
	box-sizing: border-box;
	.list{
		height: 90upx;
		line-height: 90upx;
		border-bottom: 1upx solid #e7e7e7;
		font-size: 30upx;
		.keyword{
			color: #9e9e9e;
		}
		.association{
			color: #555555;
			margin-left: 14upx;
		}
	}
}
.voice_btn{
	position: fixed;
	width: 100%;
	bottom: 22upx;
	.voice_btn_img{
		width: 103upx;
		height: 103upx;
		display: block;
		margin: 0 auto;
	}
}
.voice_title{
	font-size: 35upx;
	color: #303030;
	text-align: center;
	padding-top: 100upx;
}
.try_text{
	font-size: 25upx;
	text-align: center;
	margin-top: 35upx;
	text{
		color: #303030;
	}
	color: #7a7a7a;
}
.voice_tiao{
	margin-top: 95upx;
	margin-bottom: 164upx;
	display: flex;
	justify-content: center;
	align-items: center;
	height: 90upx;
	.voice_{
		margin: 0 8upx;
		width: 10upx;
		height: 90upx;
		background: #d81e06;
		transition: height .3s ;
	}
}
.close_ico{
	position: absolute;
	width: 50upx;
	height: 50upx;
	right: 40upx;
	top: 27upx;
}
.voice_btn2{
	width: 131upx;
	height: 131upx;
	margin: 75upx auto 144upx;
}