.store_img,.design_img,.suit_img{
	.uni-padding-wrap{
		padding: 0 22upx;
		margin-top: 20upx;
	}
	.page-section{
		border-radius: 10upx;
		overflow: hidden;
	}
	.title{
		display: flex;
		justify-content: space-between;
		padding: 0 22upx;
		margin-top: 54upx;
		.title1{
			color: #272727;
			font-size: 30upx;
			&:before{
				content: '';
				display: inline-block;
				width: 5upx;
				height: 29upx;
				border-radius: 2upx;
				background: #ff0000;
				margin-right: 8upx;
				vertical-align: middle;
			}
		}
		.title_btn{
			color: #616161;
			font-size: 28upx;
			.btn1{
				font-size: 28upx;
				color: #fff;
				background: #de0303;
				padding: 5upx 8upx;
				border-radius: 10upx;
				margin-right: 15upx;
				font-weight: bold;
				// text-shadow: 0 0 1px #000;
			}
		}
	}
	.img_container{
		display: flex;
		justify-content: space-between;
		padding: 0 22upx;
		margin-top: 20upx;
		padding-bottom:10upx;
		.img_box{
			width: 22%;
			height: 187upx;
			border-radius: 15upx;
			box-shadow: 0 0 10upx #dadada;
			.img{
				width: 100%;
				height: 100%;
			}
		}
	}
}	
.suit_img{
	.img_box{
