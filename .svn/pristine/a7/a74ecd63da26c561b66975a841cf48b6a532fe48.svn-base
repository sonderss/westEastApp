.home_circle{
	width: 708upx;
	height: 708upx;
	background: #fff;
	margin: 0upx auto 0;
	position:relative;
	.background1{
		background: #00675c;
	}
	.background2{
		background: #008537;
	}
	.left0{
		left: 0;
	}
	.right0{
		right: 0;
	}
	.top0{
		top: 0;
	}
	.bottom0{
		bottom: 0;
	}
	>view{
		width: 280upx;
		height: 280upx;
		border-radius: 20upx;
		position: absolute;
	}
	.leftTop_btn{
		background: #00675c;
		top: 0;
		left: 0;
	}
	.rightTop_btn{
		background: #008537;
		top: 0;
		right: 0;
	}
	.leftBottom_btn{
		background: #00675c;
		bottom: 0;
		left: 0;
	}
	.rightBottom_btn{
		background: #008537;
		bottom: 0;
		right: 0;
	}
	.width290{
		width: 290upx;
		height: 23upx;
		position: absolute;
		border-radius: 20upx;
		
	}
	.height290{
		width: 23upx;
		height: 290upx;
		position: absolute;
		border-radius: 20upx;
	}
	.btn_circle{
		position: absolute;
		width: 680upx;
		height: 680upx;
		background: #fff;
		border-radius:1000upx ;
		top: 50%;
		left: 50%;
		transform: translate3d(-50%,-50%,0);
	}
	.btn_circle2{
		position: absolute;
		width: 615upx;
		height: 615upx;
		// background: #fff;
		border-radius:1000upx ;
		top: 50%;
		left: 50%;
		transform: translate3d(-50%,-50%,0);
		overflow: hidden;
	}
	.half_circle{
		position: absolute;
		width: 304upx;
		height: 100%;
		border-radius: 20upx;
	}
	.fill_color1{
		background: linear-gradient(to right, #00978e , #89cfc3); 
	}
	.fill_color2{
		background: linear-gradient(to right, #35ae49 , #0a6b3c);
	}
	.line_container{
		width: 100%;
		height: 100%;
	}
	.line{
		position: absolute;
		top: 0;
		width: 2upx;
		height: 100%;
		// box-shadow: 0 0 2px #888;
		background: #fff;
		
	}
	@for $i from 1 through 6{
		.line#{$i}{
			left: 50%;
			transform-origin: 50% 50%;
			transform: rotateZ(-25.7deg*$i) translateZ(0);
		}
	}
	// @for $i from 7 through 12{
	//     .line#{$i}{
	//         left: 51.3%;
	//         transform-origin: 100% 100%;
	//         transform: rotateZ((25.7deg*$i)-180deg+25.7deg);
	//     }
	// }
	.txt1,.txt2,.txt3,.txt4{
		color: #FFFFFF;
		font-size: 35upx;
		height: 75upx;
		width: 75upx;
		// margin: 0 auto;
		position: absolute;
		.txt_p{
			text-align: center;
			display: block;
			height: 40upx;
		}
	}
	.txt1{
		transform: rotateZ(-45deg) translateZ(0);
		top: 25upx;
		left: 25upx;
	}
	.txt2{
		transform: rotateZ(45deg) translateZ(0);
		top: 25upx;
		right: 25upx;
	}
	.txt3{
		transform: rotateZ(45deg) translateZ(0);
		bottom: 40upx;
		left: 40upx;
	}
	.txt4{
		transform: rotateZ(-45deg) translateZ(0);
		bottom: 40upx;
		right: 40upx;
	}
}
