	width: 30upx;
	height: 30upx;
	border: 1upx solid #d1d1d1;
	border-radius: 50%;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	-ms-border-radius: 50%;
	-o-border-radius: 50%;
	margin-right: 10upx;
}
